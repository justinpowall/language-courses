<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Registration_Deadline_Before_End_Date</fullName>
    <active>true</active>
    <errorConditionFormula>DateTimeValue(Registration_Deadline__c) &gt; DateTimeValue(Event_End__c)</errorConditionFormula>
    <errorMessage>The Registration Deadline cannot be after the Event End Date</errorMessage>
</ValidationRule>
