<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>City_Special_Character_Restriction</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(City__c, &#39;^[^,\\%$*!.&amp;]*$&#39;))</errorConditionFormula>
    <errorDisplayField>City__c</errorDisplayField>
    <errorMessage>City can not contain any of the following characters . , \ ^ % $ * ! &amp;</errorMessage>
</ValidationRule>
