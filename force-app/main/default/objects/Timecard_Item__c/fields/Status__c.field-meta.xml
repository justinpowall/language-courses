<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status__c</fullName>
    <description>Captures status of a submitted time request.</description>
    <externalId>false</externalId>
    <inlineHelpText>Time submissions can have the following statuses: 
Not Submitted
Submitted
Pending 2nd Approval
Approved
Rejected</inlineHelpText>
    <label>Status</label>
    <required>false</required>
    <trackHistory>true</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Not Submitted</fullName>
                <default>true</default>
                <label>Not Submitted</label>
            </value>
            <value>
                <fullName>Submitted</fullName>
                <default>false</default>
                <label>Submitted</label>
            </value>
            <value>
                <fullName>Approved</fullName>
                <default>false</default>
                <label>Approved</label>
            </value>
            <value>
                <fullName>Rejected</fullName>
                <default>false</default>
                <label>Rejected</label>
            </value>
            <value>
                <fullName>Pending 2nd Approval</fullName>
                <default>false</default>
                <label>Pending 2nd Approval</label>
            </value>
            <value>
                <fullName>Rejected by 2nd Approver</fullName>
                <default>false</default>
                <label>Rejected by 2nd Approver</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
